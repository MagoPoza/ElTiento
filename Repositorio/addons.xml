<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.ElTiento.wizard" name="[COLOR gold][B]EL TIENTO[/B][/COLOR]" version="1.1" provider-name="El Tiento">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">El tiento</summary>
    <description lang="en">Addon instalador del wizard EL TIENTO para Kodi 18
</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.ElTiento.repor" name="[COLOR gold][B]EL TIENTO REPO[/B][/COLOR]" version="1.0" provider-name="ElTiento">
    <extension point="xbmc.addon.repository" name="eltientorepo">
        <info>https://raw.githubusercontent.com/MagoPoza/ElTiento/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/MagoPoza/ElTiento/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/MagoPoza/ElTiento/master/Repositorio/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repositorio wizard el tiento</summary>
        <description></description>
        <disclaimer></disclaimer>
        <platform>all</platform>
    </extension>
</addon>
</addons>
